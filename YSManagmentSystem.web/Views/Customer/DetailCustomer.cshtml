@model YSManagmentSystem.Domain.Order.Customer

@{
    Layout = null;
}




<div>
    <dl class="row">
        <dt class="col-sm-2" >
           Name:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CustomerName)
        </dd>
        <dt class="col-sm-2">
            Email:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
           Number:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ContactNumber)
        </dd>
        <dt class="col-sm-2">
            Address:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CustomerAddress)
        </dd>
    </dl>
</div>



<script>
    $(document).ready(function () {
        $('#myTableCustomer').DataTable();
    });
</script>